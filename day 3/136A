#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    int arr[n+1];
    int ans[n+1];
    int b;
    for (int i = 1; i <= n; i++) {
        cin>>arr[i];
    }
    for (int i = 1; i <= n; i++) {
        b=arr[i];
        ans[b]=i;
    }
     for (int i = 1; i <= n; i++) {
        cout<<ans[i]<<" ";
     }
    return 0;
}
